{
  "name": "gonzalo/ui5",
  "description": "backend example ui5 locally",
  "authors": [
    {
      "name": "Gonzalo Ayuso",
      "email": "gonzalo123@gmail.com"
    }
  ],
  "require": {
    "ext-bcmath": "*",
    "roave/security-advisories": "dev-master",
    "laravel/lumen": "^5.6",
    "doctrine/dbal": "^2.8",
    "php-amqplib/php-amqplib": "^2.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2"
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "./vendor/bin/phpunit --colors=always"
  }
}
